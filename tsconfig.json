{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6"],
    "strict": true,
    "sourceMap": true,
    "rootDir": ".",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules", ".vscode-test"],
  "include": ["src/**/*.ts", "test/**/*.ts", "scripts/**/*.ts"]
}
